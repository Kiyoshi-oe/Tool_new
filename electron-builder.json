  ],
  "extraMetadata": {
    "main": "public/electron.cjs",
    "description": "A resource tool for Cyrus development",
    "author": "Cyrus Team"
  },
  "win": {
    "target": "nsis",
    "icon": "public/lovable-uploads/icon_small.png"
  },
  "mac": {
    "target": "dmg",
    "icon": "public/lovable-uploads/icon_small.png"
  },
  "linux": {
    "target": "AppImage",
    "icon": "public/lovable-uploads/icon_small.png"
  },
  "npmRebuild": false,
  "electronVersion": "35.0.2",
  "buildDependenciesFromSource": true,
  "nodeGypRebuild": false,
  "electronDependencies": false
