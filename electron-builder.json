{
  "appId": "com.lovable.cyrus-resource-tool",
  "productName": "Cyrus Resource Tool",
  "description": "A resource tool for Cyrus development",
  "author": {
    "name": "Cyrus Team",
    "email": "info@cyrusteam.com"
  },
  "directories": {
